<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f2f2f2;
            color: #333;
            text-align: center;
            padding: 50px;
        }
        h1 {
            font-size: 100px;
            color: #e74c3c;
        }
        p {
            font-size: 20px;
            color: #555;
        }
        a {
            text-decoration: none;
            font-size: 18px;
            color: #3498db;
        }
        a:hover {
            color: #2980b9;
        }
        .container {
            max-width: 600px;
            margin: 0 auto;
            padding: 20px;
            background-color: #fff;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            border-radius: 8px;
        }
    </style>
</head>
<body>

    <div class="container">
        <h1>Oops</h1>
        <p><%= message %></p>
        <p>
          <a id="return-link" href="/">Return </a>
        </p>
        
    </div>
 <script>

const isAdmin = window.location.pathname.startsWith("/admin");
  document.getElementById("return-link").href = isAdmin ? "/admin/dashboard" : "/";
  document.getElementById("return-link").innerText = isAdmin ? "Return to Dashboard" : "Return to Home";
 </script>
</body>
</html>